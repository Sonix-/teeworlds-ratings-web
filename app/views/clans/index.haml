%h2 Registered clans

%table.ui.basic.table
  %thead
    %tr
      %th Rank
      %th Name
      %th Rating
      %th Players
  - @clans.each_index do |i|
    %tr
      %td= ((page - 1) * Clan.per_page) + i + 1
      %td= link_to @clans[i].name, clan_path(@clans[i])
      %td= @clans[i].rating
      %td= @clans[i].players_number
